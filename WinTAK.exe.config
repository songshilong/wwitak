  <log4net>
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <staticLogFileName value="false" />
      <datePattern value="-yyyy-MM-dd" />
      <file value="${APPDATA}/WinTAK/Logs/Log-WinTAK.txt" />
      <preserveLogFileNameExtension value="true" />
      <param name="AppendToFile" value="false" />
      <rollingStyle value="Composite" />
      <maximumFileSize value="10MB" />
      <maxSizeRollBackups value="20" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%level] :: %message%newline" />
      </layout>
    </appender>
    <appender name="ErrorFileAppender" type="log4net.Appender.RollingFileAppender">
      <staticLogFileName value="false" />
      <datePattern value="-yyyy-MM-dd" />
      <file value="${APPDATA}/WinTAK/Logs/Errors-WinTAK.txt" />
      <preserveLogFileNameExtension value="true" />
      <param name="AppendToFile" value="false" />
      <rollingStyle value="Composite" />
      <maximumFileSize value="10MB" />
      <maxSizeRollBackups value="20" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%level] :: %message%newline" />
      </layout>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ERROR" />
        <levelMax value="FATAL" />
      </filter>
    </appender>
    <appender name="FatalFileAppender" type="log4net.Appender.RollingFileAppender">
      <staticLogFileName value="false" />
      <datePattern value="-yyyy-MM-dd" />
      <file value="${APPDATA}/WinTAK/Logs/Fatal-WinTAK.txt" />
      <preserveLogFileNameExtension value="true" />
      <param name="AppendToFile" value="false" />
      <rollingStyle value="Composite" />
      <maximumFileSize value="10MB" />
      <maxSizeRollBackups value="20" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%level] :: %message%newline" />
      </layout>
      <filter type="log4net.Filter.LevelMatchFilter">
        <acceptOnMatch value="true" />
        <levelToMatch value="FATAL" />
      </filter>
      <filter type="log4net.Filter.DenyAllFilter" />
    </appender>
    <appender name="TraceAppender" type="log4net.Appender.TraceAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="[%level] :: %message%newline" />
      </layout>
    </appender>
    <root>
      <level value="Info" />
      <appender-ref ref="LogFileAppender" />
      <appender-ref ref="ErrorFileAppender" />
      <appender-ref ref="FatalFileAppender" />
      <appender-ref ref="TraceAppender" />
    </root>
  </log4net>
